  <program>
print("Cylinder Volume Calculator")
radius = input("Enter the radius of the base of cylinder (cm): ")
radius = float(radius)
height = input("Enter the height of cylinder (cm): ")
height = float(height)

volume = 3.14159 * radius * radius * height

print("Volume of the cylinder = ", volume, "cubic cm")


  </program>
